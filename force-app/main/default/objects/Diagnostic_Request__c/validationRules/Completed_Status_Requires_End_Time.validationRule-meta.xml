<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Completed_Status_Requires_End_Time</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
 ISPICKVAL(Status__c, &apos;Completed&apos;),
 ISBLANK(End_Time__c)
)</errorConditionFormula>
    <errorDisplayField>End_Time__c</errorDisplayField>
    <errorMessage>Completed status requires end time</errorMessage>
</ValidationRule>
