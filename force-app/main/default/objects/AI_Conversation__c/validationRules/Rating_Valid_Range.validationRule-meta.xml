<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Rating_Valid_Range</fullName>
    <active>true</active>
    <errorConditionFormula>OR(
       Satisfaction_Rating__c &lt; 1,
       Satisfaction_Rating__c &gt; 5
     )</errorConditionFormula>
    <errorMessage>Satisfaction Rating must be between 1 and 5</errorMessage>
</ValidationRule>
